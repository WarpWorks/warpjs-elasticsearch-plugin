{{#partial "content"}}
  <br />
  <div class="row search-results">
    <div class="col-xs-1 left-right-margin"></div>
    <div class="col-md-10">
      <div class="row">
        <div class="col-md-6 summary">
          {{#if withSearch}}
            {{> summary}}
          {{/if}}
        </div>
        <div class="col-md-6 search-box">
          {{> search-box}}
        </div>
      </div>
      {{#if total}}
        <div class="row">
          <div class="col-xs-12"><hr /></div>
        </div>
        <div class="row search-results-items">
          {{> results results=_embedded.results}}
        </div>
        <div class="row">
          {{> pagination pages=_embedded.pages}}
        </div>
      {{/if}}
    </div>
    <div class="col-xs-1 left-right-margin"></div>
  </div>
{{/partial}}

{{> portal-index-page}}

